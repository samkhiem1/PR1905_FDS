<% provide(:title, "Categories") %>
<%= link_to "Create Category", new_admins_category_path, class: "btn btn-primary"%>
<% @categories.each do |category| %>
  <%= category.name %>
  <%= link_to "edit category", edit_admins_category_path(category.id), class: "btn btn-primary"%>
  <%= link_to "delete", admins_category_path(category), method: :delete, data: { confirm: "You sure?"}, class: "btn btn-primary" %>
<% end %>
